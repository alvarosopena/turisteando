<!DOCTYPE html>
<html lang="en">
  <head>
      <%- include('partials/head.ejs') %>
      <link rel="stylesheet" href="/css/partners.css">
      <link rel="stylesheet" href="/css/seccion.css">
      <link rel="stylesheet" href="/css/indexhome.css">
  
  </head>

  <body>
    <header>
      <%- include('partials/header.ejs') %>
    </header>
    <main>
      <section class ="hero1">
      <div>
        <nav class="content-secction">
          <ul>
           
            <% if(user) { %> 
            <% if (user.category_id == 1){ %>
              <li><a href="/index-admin" class="seccion">Secci√≥n para administradores</a>
              <hr>
            <% } %>
            <% } %> 
                            
              <li><a href="/user/<%= ( user ? user.id : "../login") %> " class="seccion"> Tu perfil de usuario </a> </li>
           </ul>
        </nav>
      </div>

      <!--API-->
      <section> 
          <!--  <div><img class="content-imagefirst"src="images/destinos/mund.jpg"></div>  -->
          
      </section>

     
       
        <div class="texto-barra">
          <h3> Tu aventura a unos clicks de distancia </h3>
        </div>
        <br>
      <br>
        <!-- barra de busqueda -->
      <section>
        <br>
        <div class="pic-container1"> 
          <div class="box">
            <br>
            <div class="container-3">
            <!-- <form action="search" method="GET"> -->
              <span class="icon"><a href="/"> Ir <i class="fa fa-search"></i></a></span>
              <input type="search" id="search" placeholder="Busca tu proximo destino..." />
            </div>
          </div>
        </div>
        <br>
       
      </section> 
    </section>

      <!--  DESDE ACA PRODUCTOS DESCATADOS        -->
<div class="container">
      <section class="first">
        <div class="containerproducts" >  
          <!-- <div class="pd"> -->
            <div class="texto-titles">
              <h3> Destinos destacados </h3>
            </div>
         <!--  </div> -->
            <div class="destacado">
             <div class ="listaProducto">
              <%products.forEach(product => { %>
                <% if (product?.destacado === "SI"){ %>
                  <div class="producto">
                    <div class="producto-destacado">
                    <h1 class="title"><%= product.title%></h1>
                      <div class="content">
                        <a href="/lista/<%= product.id%>" target="_blank">
                          <div class="content-overlay"> <!-- PUEDE IR OTRO TEXTO --></div>
                            <div><img class="content-images-destacado" src="images/destinos/<%=product.image%>"></div>
                            <div class="ribbon ribbon-top-left"><span><i class="fas fa-star"></i><i class="fas fa-star"></i><i class="fas fa-star"></i><i class="fas fa-star"></i><i class="fas fa-star"></i></span></div>
                            <div class="content-details fadeIn-bottom"> <!-- hace que salga la info de abajo -->
                              <h3 class="content-title"><%= product.description%></h3>
                              <p class="content-text"> ARS $: <%= product.price%> p/p</p>
                              <a href="/lista/<%= product.id%>" style="color: blanchedalmond;">VER DETALLE</a>
                            </div>
                        </a>
                      </div>
                    </div>
                  </div>
                <% } %>
              <% }) %>
            </div>
            <br>
            <hr>
            <br>
          </div>
          
          <!--  HASTA ACA PRODUCTOS DESCATADOS        -->
          <div class="texto-titles">
            <h3> Otros destinos </h3>
          </div>
            <div class="destacado">
              <div class ="listaProducto">
                <%products.forEach(product => { %>
                  <% if (product?.destacado != "SI"){ %>
                    <div class = "producto" >
                      <h1 class="title"><%= product.title%></h1>
                        <div class="content">
                          <a href="/lista/<%= product.id%>" target="_blank">
                            <div class="content-overlay"> <!-- PUEDE IR OTRO TEXTO --></div>
                            <div><img class="content-images" src="images/destinos/<%=product.image%>"></div>
                            <div class="content-details fadeIn-bottom"> <!-- hace que salga la info de abajo -->
                              <h3 class="content-title"><%= product.description%></h3>
                              <p class="content-text"> ARS $: <%= product.price%> p/p</p>
                              <a href="/lista/<%= product.id%>" style="color: blanchedalmond;">VER DETALLE</a>
                            </div>
                          </a>
                        </div>
                    </div>
                    
                  <% } %>
                <% }) %>
              </div>
            </div>
        </div>
        <br>
      </div>     
      </section>

      <section class="partners"> 
        <div class="partners">
          <div class="texto-titles" style="font-size: 25px;">
            <h3> Colaboran con nosotros </h3>
          </div>
            <div class="box-partner">
              <h3 class="title">LATAM</h3>
                <div class="content">
                  <a href="https://www.latam.com/es_ar/" target="_blank">
                    <div class="content-overlay"></div>
                     <img class="content-image" src="data:image/png;base64,">
                      <div class="content-details fadeIn-bottom">
                        <h3 class="content-title">LATAM</h3>
                        <p class="content-text"></p>
                      </div>
                  </a>
               </div>
            </div>
          <div class="box-partner">
            <h3 class="title">HOSTELWORLD</h3>
              <div class="content">
               <a href="https://www.hostelworld.com/" target="_blank">
                  <div class="content-overlay"></div>
                    <img class="content-image" src="data:image/png;base64,">
                     <div class="content-details fadeIn-bottom">
                      <h3 class="content-title">HOSTELWORLD</h3>
                      <p class="content-text">Meet the world</p>
                      </div>
                  </a>
              </div>
          </div>
          <div class="box-partner">
            <h3 class="title">Assist Card</h3>
              <div class="content">
               <a href="https://www.assistcard.com/ar" target="_blank">
                  <div class="content-overlay"></div>
                    <img class="content-image" src="data:image/png;base64,">
                     <div class="content-details fadeIn-bottom">
                      <h3 class="content-title">Assist Card</h3>
                      <p class="content-text">El mejor seguro para tu viaje</p>
                      </div>
                  </a>
              </div>
          </div>
        </div>
      </div>
      </section>
    </main>

    <footer>
      <%- include('partials/footer.ejs') %>
    </footer>

  </body>
</html>